{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "inputs": ["$TURBO_DEFAULT$"],
      "outputs": [".cache/eslint", ".cache/tsconfig.tsbuildinfo"]
    },
    "test": {
      "inputs": ["$TURBO_DEFAULT$"],
      "outputs": [".cache/jest/**"]
    }
  },
  "ui": "tui"
}
